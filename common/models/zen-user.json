    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "rh",
      "permission": "ALLOW",
      "property": "getCandidat"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "itw",
