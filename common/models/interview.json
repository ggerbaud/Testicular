{
  "name": "Interview",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "number",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "quizzAttempts": {
      "type": "hasMany",
      "model": "QuizzAttempt",
      "foreignKey": "interviewId"
    },
    "user": {
      "type": "belongsTo",
      "model": "ZenUser"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "root",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
