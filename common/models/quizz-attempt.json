{
  "name": "QuizzAttempt",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "state": {
      "type": "number",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "attempts": {
      "type": "hasMany",
      "model": "Attempt",
      "foreignKey": "quizzAttemptId"
    },
    "quizz": {
      "type": "belongsTo",
      "model": "Quizz",
      "foreignKey": "quizzId"
    },
    "interview": {
      "type": "belongsTo",
      "model": "Interview"
    },
    "owner": {
      "type": "belongsTo",
      "model": "ZenUser",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "root",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "quizzInfo"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "quizzQuestions"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__attempts"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__updateById__attempts"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "validate"
    }
  ],
  "methods": []
}
